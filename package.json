    "taboverride": "^4.0.3"
  },
  "devDependencies": {
    "@deskpro/apps-dpat": "^0.10.0",
    "@deskpro/apps-sdk-core": "^1.0.0-beta.26",
    "@deskpro/apps-sdk-react": "0.2.8",
    "babel-eslint": "^8.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
